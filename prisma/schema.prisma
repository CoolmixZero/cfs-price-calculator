generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model Cars {
  id           String    @id @default(auto()) @map("_id") @db.ObjectId
  title        String @unique
  image        String?
  max_speed    Int?
  acceleration Float?
  min_price    Int?
  max_price    Int?
}
